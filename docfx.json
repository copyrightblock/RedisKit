{
  "metadata": [
    {
      "src": [
        {
          "src": "./",
          "files": [
            "RedisKit/**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "globalNamespaceId": "RedisKit",
      "filter": "filterConfig.yml",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "QWEN.md",
          "CRUSH.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "*.png",
          "*.ico"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "RedisKit",
      "_appName": "RedisKit",
      "_appFooter": "RedisKit - High-performance Redis toolkit for .NET",
      "_appLogoPath": "logo.png",
      "_appFaviconPath": "favicon.ico",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/ersintarhan/RedisKit",
        "branch": "main"
      }
    }
  }
}